{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"prettier.tabWidth": 4,
	"prettier.endOfLine": "auto",
	"prettier.useTabs": true,
	"prettier.trailingComma": "none",
	"cSpell.words": [
		"fontawesome",
		"fortawesome",
		"homeserver",
		"openpgp",
		"svgs"
	],
	"[scss]": {
		"editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
	}
}